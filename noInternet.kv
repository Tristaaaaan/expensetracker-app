<NoInternet>:
  name: "noInternet"
  MDBoxLayout:
    size_hint: 1, 1
    orientation: "vertical"
    pos_hint: { "center_x": .5, "center_y": .5 }
    MDLabel:
      id: internetData
      text: 'No Internet'
      halign: 'center'
    MDFlatButton:
      text: 'Try again.'
      on_release:
        root.manager.current = 'loading'


